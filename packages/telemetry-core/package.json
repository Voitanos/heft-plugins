{
  "name": "@voitanos/heft-plugins-telemetry-core",
  "description": "Shared telemetry client for @voitanos Heft plugins.",
  "version": "0.1.0-beta.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Voitanos/heft-plugins.git",
    "directory": "packages/telemetry-core"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "type": "module",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "scripts": {
    "build": "heft build --clean",
    "build:inject": "npm run build && export HEFT_PLUGINS_APP_INSIGHTS_CONNECTION_STRING=$(op read 'op://Private/heft-plugins Azure App Insights/connection string') && op run -- node scripts/inject-connection-string.js",
    "build:prod": "npm run build && npm run set-azappinsights-connection-string && npm run inject-azappinsights-connection-string",
    "set-azappinsights-connection-string": "export HEFT_PLUGINS_APP_INSIGHTS_CONNECTION_STRING='op://Private/heft-plugins Azure App Insights/connection string'",
    "inject-azappinsights-connection-string": "op run -- node scripts/inject-connection-string.js",
    "test": "heft test --clean",
    "start": "heft build-watch"
  },
  "dependencies": {
    "applicationinsights": "^2.9.5",
    "ci-info": "^4.0.0"
  },
  "devDependencies": {
    "@rushstack/heft": "~1.1.5",
    "@rushstack/heft-jest-plugin": "~1.1.5",
    "@rushstack/heft-typescript-plugin": "~1.1.5",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.19.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.0",
    "typescript": "~5.7.2"
  },
  "keywords": [
    "telemetry",
    "application-insights",
    "heft",
    "heft-plugins",
    "voitanos"
  ],
  "homepage": "https://github.com/Voitanos/heft-plugins",
  "issues": "https://github.com/Voitanos/heft-plugins/issues",
  "author": "Andrew Connell <andrew@voitanos.io>",
  "maintainers": [
    {
      "name": "andrewconnell",
      "email": "andrew@voitanos.io"
    }
  ]
}
